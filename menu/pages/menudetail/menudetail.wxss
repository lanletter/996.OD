}

.show .ulist .list .titlepic {
  margin: -1rpx -1rpx 0 -1rpx;
  width: 306rpx;
  height: 304rpx;
  border-radius: 16rpx 16rpx 0px 0rpx;
}

.show .ulist .list .userinfo {
  margin-top: 18rpx;
  height: 48rpx;
}

.show .ulist .list .userinfo image {
  float: left;
  height: 48rpx;
  width: 48rpx;
  border-radius: 50%;
