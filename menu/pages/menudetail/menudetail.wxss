  height: 44rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: rgba(214, 214, 214, 1);
  line-height: 44rpx;
  text-align: center;
}

.show .ulist {
  margin-top: 20rpx;
  width: 100%;
  height: 540rpx;
  white-space: nowrap;
}

.show .ulist .list {
  box-sizing: border-box;
  display: inline-block;
  width: 306rpx;
  height: 538rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 16rpx;
  border: 1rpx solid #ededed;
  margin-right: 28rpx;
