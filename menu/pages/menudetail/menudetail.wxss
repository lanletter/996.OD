}

.show .ulist .list .userinfo text {
  float: left;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(154, 154, 154, 1);
  height: 48rpx;
  line-height: 48rpx;
}

.show .ulist .list .content {
  margin: 20rpx 36rpx 20rpx 30rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: rgba(31, 31, 31, 1);
  line-height: 34rpx;
  width: 240rpx;
  height: 68rpx;
  white-space: normal;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.show .ulist .list .like {
  position: absolute;
  left: 20rpx;
  bottom: 16rpx;
}

.show .ulist .list .like image {
  float: left;
  width: 48rpx;
  height: 48rpx;
}

.show .ulist .list .like text {
  float: left;
  margin-left: 8rpx;
  height: 48rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(200, 175, 112, 1);
  line-height: 48rpx;
}

