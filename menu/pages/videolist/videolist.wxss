  display: flex;
  flex-direction: column;
  justify-content: center;
}

.videobox {
  width: 100%;
  height: 100%;
}

.collect{
  position: absolute;
  top:40rpx;
  right: 40rpx;
  width: 72rpx;
  height: 72rpx;
  background: #ffffff;
