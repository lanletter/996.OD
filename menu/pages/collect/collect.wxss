.searchbar1 {
  width: 100%;
  margin-top: 40rpx;
  padding: 40rpx 50rpx;
  border-bottom: 1px solid #d6d6d6;
  position: relative;
}

.searchbar1 input {
  height: 44rpx;
  width: 600rpx;
  font-size: 48rpx;
  font-family: PingFangSC-Semibold;
  font-weight: 600;
  color: rgba(31, 31, 31, 1);
  line-height: 44rpx;
}

.searchbar1 .placeholder {
  color: rgba(200, 175, 112, 1);
  border-left: 1px solid #d6d6d6;
  padding-left: 6rpx;
}

.searchbar2 {
  width: 670rpx;
  height: 88rpx;
  margin: 40rpx 40rpx 0 40rpx;
 background:rgba(244,244,244,1);
border-radius:49rpx;
  position: relative;
}

.searchbar2 .icon{
  float: left;
  width: 48rpx;
  height: 48rpx;
  margin: 20rpx 22rpx 20rpx 40rpx;
}

.searchbar2 input {
  float: left;
  height: 88rpx;
  width: 500rpx;
  font-size:28rpx;
font-family:PingFangSC-Medium;
font-weight:480;
  color: rgba(31, 31, 31, 1);
  line-height: 88rpx;
}

.searchbar2 .placeholder {
 font-size:28rpx;
font-family:PingFangSC-Medium;
font-weight:500;
color:rgba(178,178,178,1);
}

.deleteinputvalue{
  position: absolute;
  top:86rpx;
  right: 60rpx;
  width: 32rpx;
  height: 32rpx;
}
.deleteinputvalue2{
  position: absolute;
  top:72rpx;
  right: 78rpx;
  width: 32rpx;
  height: 32rpx;
}

.tiplist {
  margin-top: 6rpx;
}

.tiplist view {
  margin-left: 30rpx;
  padding: 13rpx 0 16rpx 0;
  border-bottom: 1px solid #ededed;
}

.tiplist view:last-child {
  border: 0;
}

.tiplist .clock {
  width: 48rpx;
  height: 48rpx;
  vertical-align: middle;
}

.tiplist .deleteword{
  float: right;
  width: 32rpx;
  height: 32rpx;
  margin-top: 10rpx;
  margin-right: 40rpx;
}

.tiplist view text {
  font-size: 32rpx;
  font-family: PingFangSC-Semibold;
  font-weight: 600;
  color: rgba(102, 102, 102, 1);
  line-height: 48rpx;
  margin-left: 12rpx;
}

.hots {
  padding: 80rpx 40rpx;
  border-top: 1px solid #ededed;
}

.hots .title {
  height: 50rpx;
  font-size: 36rpx;
  font-family: PingFangSC-Semibold;
  font-weight: 600;
  color: rgba(31, 31, 31, 1);
  line-height: 50rpx;
}

.hots .hotlist {
  margin-top: 60rpx;
}

.hots .hotlist view {
  float: left;
  height: 60rpx;
  background: rgba(237, 237, 237, 1);
  border-radius: 30rpx;
  padding: 0 32rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
  line-height: 60rpx;
  margin: 0 24rpx 24rpx 0;
}

/* 搜索结果 */
.tabbar{
  width: 100%;
  margin-top: 24rpx;
  height: 72rpx;
  border-bottom: 1rpx solid #EDEDED;
  padding-left: 40rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
}
.tabbar .tabs{
  margin-right: 68rpx;
  font-size:28rpx;
line-height:72rpx;
text-align: center;
}
.tabbar .tab1{
font-family:PingFangSC-Medium;
font-weight:500;
color:rgba(31,31,31,1);
border-bottom: 3px solid #C8AF70;
 height: 71rpx; 
}
.tabbar .tab2{
  font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(154,154,154,1);
}

.ulist .listmenu{
 margin-top: 40rpx;
}
.ulist .listmenu .list {
  position: relative;
  margin: 0 40rpx 60rpx 40rpx;
  width: 670rpx;
  overflow: hidden;
}

.ulist .listmenu .headpic {
  width: 670rpx;
  height: 376rpx;
  border-radius: 12rpx;
}

.ulist .listmenu .info {
  margin-top: 18rpx;
  margin-left: 26rpx;
}

.ulist .listmenu .info .titlemin {
  display: block;
  width: 600rpx;
  height: 50rpx;
  font-size: 36rpx;
  font-family: PingFangSC-Semibold;
  font-weight: 600;
  color: rgba(31, 31, 31, 1);
  line-height: 50rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.ulist .listmenu .info .icons {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  margin-top: 16rpx;
  height: 32rpx;
}

.ulist .listmenu .info .icons .pageview {
  margin-right: 32rpx;
}

.ulist .listmenu .info .icons .icon image {
  float: left;
  height: 32rpx;
  width: 32rpx;
  vertical-align: top;
  margin-right: 6rpx;
}

.ulist .listmenu .info .icons .icon text {
  float: left;
  height: 32rpx;
  line-height: 32rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(154, 154, 154, 1);
}

.ulist .listvideo{
  margin-top: 40rpx;
  margin-left: 40rpx;
}
.ulist .listvideo .list {
  float: left;
  width: 320rpx;
  margin-right: 30rpx;
  margin-bottom: 60rpx;
  position: relative;
}

.ulist .listvideo .list .picture {
  width: 320rpx;
  height: 240rpx;
  border-radius: 16rpx;
}

.ulist .listvideo .list .titlemin {
  width: 320rpx;
  height: 100rpx;
  font-size: 36rpx;
  font-family: PingFangSC-Semibold;
  font-weight: 600;
  color: rgba(31, 31, 31, 1);
  line-height: 50rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.ulist .listvideo .list .time {
  display: block;
  position: absolute;
  left: 20rpx;
  top: 172rpx;
  width: 148rpx;
  height: 48rpx;
  line-height: 48rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  background: rgba(0, 0, 0, 0.3);
  border-radius: 8rpx;
}

.ulist .listvideo .list .time .icon {
  display: block;
  width: 48rpx;
  height: 48rpx;
  float: left;
  margin-right: 10rpx;
}

.tabselect {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 165rpx;
  border-top: 1px solid #ededed;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  background: #fff;
}

.tabselect .tabbtn {
  width: 50%;
  height: 100%;
  display: flex;
  flex-direction: column;
}

.tabselect .icon {
  margin: 10rpx auto;
  margin-bottom: 0;
  width: 48rpx;
  height: 48rpx;
}

.tabselect .txt {
  margin-top: 16rpx;
  height: 28rpx;
  font-size: 20rpx;
  font-family: PingFangSC-Semibold;
  font-weight: 600;
  line-height: 28rpx;
  text-align: center;
}

.tabselect .txtgold {
  color: rgba(200, 175, 112, 1);
}

.tabselect .txtgrey {
  color: #d6d6d6;
}
